{
  "name": "valid-fluent",
  "version": "0.1.0",
  "type": "module",
  "description": "A strongly-typed, chainable validation library for TypeScript. Craft intricate validation rules with Fluent API goodness. Go beyond boilerplate and embrace customizability.",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "lint": "npx rome check ./src",
    "format": "npx rome format ./src --write",
    "build": "rollup -c && node clean-dist.js"
  },
  "keywords": [
    "validation",
    "fluent",
    "typescript",
    "form-validation",
    "data-validation",
    "type-safety",
    "model-validation",
    "backend-validation",
    "frontend-validation",
    "custom-rules",
    "fluent-api"
  ],
  "author": "Marcel Wenner (https://github.com/nbyx)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbyx/valid-fluent.git"
  },
  "bugs": {
    "url": "https://github.com/nbyx/valid-fluent/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.6.4",
    "rollup": "^3.29.3",
    "rollup-plugin-delete": "^2.0.0",
    "rome": "^12.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.5"
  }
}
